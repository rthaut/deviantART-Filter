    "gulp-concat": "^2.6.1",
    "gulp-ejs": "^3.1.3",
    "gulp-eslint": "^4.0.0",
    "gulp-header": "^2.0.5",
    "gulp-if": "^2.0.2",
    "gulp-less": "^4.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-merge-json": "^1.3.1",
